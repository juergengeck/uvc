{
  "name": "uvc",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android-debug": "npx expo start --android",
    "ios": "expo run:ios",
    "ios-debug": "npx expo start --ios",
    "web": "expo start --web",
    "web-debug": "npx expo start --web",
    "setup-llama": "node scripts/setup-llama-rn.js",
    "setup-modules": "node scripts/setup-llama-rn.js",
    "prebuild:clean": "rm -rf ios android && expo prebuild --clean --non-interactive && npm run setup-modules && npm run pod-install",
    "prebuild": "expo prebuild --non-interactive && npm run setup-modules && npm run pod-install",
    "clean": "rm -rf ios/Pods android/app/build",
    "cleanup-llama": "echo 'Llama.rn cleanup already performed - see docs/llama-rn-usage.md'",
    "pod-install": "cd ios && pod install --repo-update && cd ..",
    "build:ios": "npm run prebuild:clean && expo run:ios",
    "build:ios:with-fix": "npm run build:ios",
    "build:android": "npm run prebuild:clean && expo run:android",
    "simulator": "npm run build:ios -- --simulator 'iPhone 15'",
    "test": "jest --watchAll",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prestart": "node init-dev.mjs",
    "postinstall": "patch-package && node scripts/fix-refinio-packages.js"
  },
  "dependencies": {
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/runtime": "^7.28.2",
    "@expo/cli": "^0.24.20",
    "@expo/config-plugins": "~10.1.1",
    "@expo/metro-config": "^0.21.7",
    "@expo/metro-runtime": "~5.0.4",
