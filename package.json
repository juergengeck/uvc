  "private": true,
  "codegenConfig": {
    "name": "AppSpecs",
    "type": "modules",
    "jsSrcsDir": "specs"
  },
  "expo": {
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchitectureEnabled": true
          },
          "android": {
            "newArchitectureEnabled": true
          }
        }
      ]
    ],
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-event-listeners",
          "react-native-fs",
          "i18next",
          "@rneui/base"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "overrides": {
    "@expo/config-plugins": "~10.1.1",
    "expo": "53.0.22",
    "patch-package": {
      "tmp": "0.2.5"
    },
    "react-native-markdown-display": {
      "markdown-it": "12.3.2"
    }
  },
  "resolutions": {
    "expo-dev-client": "~5.1.8",
    "expo-dev-menu": "~6.1.10",
    "expo-dev-menu-interface": "~1.10.0",
    "@expo/config-plugins": "~10.1.1",
    "@expo/prebuild-config": "~9.0.0",
    "@expo/metro-config": "^0.20.14"
  },
  "jest": {
    "preset": "jest-expo"
  }
