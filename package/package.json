  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refinio/one.health.git"
  },
  "keywords": [
    "health",
    "apple-health",
    "google-fit",
    "fhir",
    "hl7",
    "one-platform",
    "healthcare",
    "medical-records"
  ],
  "author": "Refinio",
  "license": "MIT",
  "dependencies": {
    "react-native-health": "^1.18.0",
    "react-native-google-fit": "^0.19.1",
    "fhir": "^4.11.1",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/fhir": "^0.0.36",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.0"
  },
  "files": [
    "src/**/*",
    "README.md",
